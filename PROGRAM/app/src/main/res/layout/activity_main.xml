            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.979"
            app:layout_constraintHorizontal_bias="0.977"/>
    <Button
            android:text="Nyomd meg, ha meleg vagy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button" tools:layout_editor_absoluteY="737dp"
            tools:layout_editor_absoluteX="0dp" android:visibility="visible" tools:visibility="visible"
            android:backgroundTint="#00FFD9"/>

</androidx.constraintlayout.widget.ConstraintLayout>